{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/${archives_base_name}",
    "sources": "https://github.com/${author}/${mod_id}",
    "issues": "https://github.com/${author}/${mod_id}/issues"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/H8xZpfscCg"
      }
    }
  },
  "license": "GPLv3+",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.craftish37.commonboat.CommonBoat"
    ],
    "modmenu": [
      "net.craftish37.commonboat.CommonBoatModMenuIntegration"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${min_fabricloader_version}",
    "fabric": ">=${min_fabric_version}",
    "minecraft": ">=${min_minecraft_version} <=${minecraft_version}"
  },
  "suggests": {
    "modmenu": "*",
    "cloth-config": "*"
  }
}